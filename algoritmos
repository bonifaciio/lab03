lass Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
        self.head = None

    def append(self, data):
        new_node = Node(data)
        if not self.head:
            self.head = new_node
            return
        last = self.head
        while last.next:
            last = last.next
        last.next = new_node

    def count_nodes(self):
        count = 0
        current = self.head
        while current:
            count += 1
            current = current.next
        return count

lista = LinkedList()
lista.append(10)
lista.append(20)
lista.append(30)
lista.append(40)
lista.append(45)

print("NÃºmero de nodos:", lista.count_nodes())
